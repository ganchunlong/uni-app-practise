<view><news-nav-bar vue-id="1" tabBars="{{tabBars}}" tabIndex="{{tabIndex}}" data-event-opts="{{[['^changTab',[['changTab']]]]}}" bind:changTab="__e"></news-nav-bar><view class="uni-tab-bar"><swiper class="swiper-box" style="{{'height:'+(swiperheight+'px')+';'}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['tabChange',['$event']]]]]}}" bindchange="__e"><swiper-item><scroll-view class="list" scroll-y data-event-opts="{{[['scrolltolower',[['loadmore',['$event']]]]]}}" bindscrolltolower="__e"><block s-for="{{guanzhu.list}}" s-for-item="item" s-for-index="index" s-key="index"><block><common-list vue-id="{{'2-'+index}}" item="{{item}}" index="{{index}}"></common-list></block></block><load-more vue-id="3" loadtext="{{guanzhu.loadtext}}"></load-more></scroll-view></swiper-item><swiper-item><scroll-view class="list" scroll-y><view class="search-input"><input class="uni-input" placeholder="搜索内容" placeholder-class="icon iconfont icon-sousuo topic-search"/></view><swiper class="topic-swiper" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block s-for="{{topic.swiper}}" s-for-item="item" s-for-index="index" s-key="index"><block><swiper-item><image src="{{item.src}}" mode="widthFix" lazy-load></image></swiper-item></block></block></swiper><topic-nav vue-id="4" nav="{{topic.nav}}"></topic-nav><view class="topic-new"><view>最近更新</view><block s-for="{{topic.list}}" s-for-item="item" s-for-index="index" s-key="index"><block><topic-list vue-id="{{'5-'+index}}" item="{{item}}" index="{{index}}"></topic-list></block></block></view></scroll-view></swiper-item></swiper></view></view>