<view><news-nav-bar vue-id="1" tabBars="{{tabBars}}" tabIndex="{{tabIndex}}" data-event-opts="{{[['^changTab',[['changTab']]]]}}" onChangTab="__e" onVueInit="__l"></news-nav-bar><view class="uni-tab-bar"><swiper class="swiper-box" style="{{'height:'+(swiperheight+'px')+';'}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['tabChange',['$event']]]]]}}" onChange="__e"><swiper-item><scroll-view class="list" scroll-y data-event-opts="{{[['scrollToLower',[['loadmore',['$event']]]]]}}" onScrollToLower="__e"><block a:for="{{guanzhu.list}}" a:for-item="item" a:for-index="index" a:key="index"><block><common-list vue-id="{{'2-'+index}}" item="{{item}}" index="{{index}}" onVueInit="__l"></common-list></block></block><load-more vue-id="3" loadtext="{{guanzhu.loadtext}}" onVueInit="__l"></load-more></scroll-view></swiper-item><swiper-item><scroll-view class="list" scroll-y><view class="search-input"><input class="uni-input" placeholder="搜索内容" placeholder-class="icon iconfont icon-sousuo topic-search"/></view><swiper class="topic-swiper" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block a:for="{{topic.swiper}}" a:for-item="item" a:for-index="index" a:key="index"><block><swiper-item><image src="{{item.src}}" mode="widthFix" lazy-load></image></swiper-item></block></block></swiper><topic-nav vue-id="4" nav="{{topic.nav}}" onVueInit="__l"></topic-nav><view class="topic-new"><view>最近更新</view><view class="topic-list u-f"><image src="../../static/demo/topicpic/15.jpeg" mode="widthFix" lazy-load></image><view><view>#话题名称#</view><view>话题描述</view><view>动态 10 今日 5</view></view></view></view></scroll-view></swiper-item></swiper></view></view>